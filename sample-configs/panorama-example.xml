<?xml version="1.0"?>
<config version="6.0.0" urldb="brightcloud">
  <mgt-config>
    <devices/>
    <users>
      <entry name="admin">
        <phash>fnRL/G5lXVMug</phash>
        <permissions>
          <role-based>
            <superuser>yes</superuser>
          </role-based>
        </permissions>
      </entry>
    </users>
  </mgt-config>
  <shared>
    <address>
      <entry name="shared-host1">
        <ip-netmask>1.2.2.3/32</ip-netmask>
        <tag>
          <member>grp.shared-group1</member>
        </tag>
      </entry>
      <entry name="shared-host2">
        <ip-netmask>14.5.6.7/32</ip-netmask>
        <tag>
          <member>grp.shared-group1</member>
        </tag>
      </entry>
      <entry name="Datacenter Subnet">
        <ip-netmask>10.0.0.0/16</ip-netmask>
      </entry>
    </address>
    <address-group>
      <entry name="shared-group1">
        <static>
          <member>shared-host1</member>
          <member>shared-host2</member>
          <member>test-group2</member>
        </static>
      </entry>
      <entry name="test-dynamic">
        <dynamic>
          <filter>'grp.shared-group1'</filter>
        </dynamic>
      </entry>
      <entry name="test-group2">
        <static>
          <member>shared-host2</member>
        </static>
      </entry>
    </address-group>
    <tag>
      <entry name="grp.local-group1"/>
      <entry name="grp.shared-group1"/>
    </tag>
    <pre-rulebase>
      <decryption>
        <rules>
          <entry name="shared pre decrypt 1">
            <category>
              <member>any</member>
            </category>
            <type>
              <ssl-forward-proxy/>
            </type>
            <from>
              <member>any</member>
            </from>
            <to>
              <member>any</member>
            </to>
            <source>
              <member>any</member>
            </source>
            <destination>
              <member>any</member>
            </destination>
            <source-user>
              <member>any</member>
            </source-user>
            <action>no-decrypt</action>
          </entry>
        </rules>
      </decryption>
      <security>
        <rules>
          <entry name="shared pre rule 1">
            <from>
              <member>any</member>
            </from>
            <to>
              <member>any</member>
            </to>
            <source>
              <member>any</member>
            </source>
            <destination>
              <member>any</member>
            </destination>
            <source-user>
              <member>any</member>
            </source-user>
            <category>
              <member>any</member>
            </category>
            <application>
              <member>any</member>
            </application>
            <service>
              <member>application-default</member>
            </service>
            <hip-profiles>
              <member>any</member>
            </hip-profiles>
            <action>allow</action>
          </entry>
        </rules>
      </security>
    </pre-rulebase>
    <post-rulebase>
      <security>
        <rules>
          <entry name="shared post rule 1">
            <from>
              <member>any</member>
            </from>
            <to>
              <member>any</member>
            </to>
            <source>
              <member>any</member>
            </source>
            <destination>
              <member>any</member>
            </destination>
            <source-user>
              <member>any</member>
            </source-user>
            <category>
              <member>any</member>
            </category>
            <application>
              <member>any</member>
            </application>
            <service>
              <member>application-default</member>
            </service>
            <hip-profiles>
              <member>any</member>
            </hip-profiles>
            <action>allow</action>
          </entry>
        </rules>
      </security>
    </post-rulebase>
    <profile-group>
      <entry name="Shared Production Profile"/>
    </profile-group>
  </shared>
  <devices>
    <entry name="localhost.localdomain">
      <deviceconfig>
        <system>
          <ip-address>192.168.50.11</ip-address>
          <netmask>255.255.255.0</netmask>
          <update-server>updates.paloaltonetworks.com</update-server>
          <update-schedule>
            <threats>
              <recurring>
                <weekly>
                  <day-of-week>wednesday</day-of-week>
                  <at>01:02</at>
                  <action>download-only</action>
                </weekly>
              </recurring>
            </threats>
          </update-schedule>
          <timezone>Europe/Paris</timezone>
          <service>
            <disable-telnet>yes</disable-telnet>
            <disable-http>yes</disable-http>
          </service>
          <hostname>Panorama</hostname>
          <default-gateway>192.168.50.2</default-gateway>
          <dns-setting>
            <servers>
              <primary>8.8.8.8</primary>
            </servers>
          </dns-setting>
        </system>
      </deviceconfig>
      <device-group>
        <entry name="Perimeter-FWs">
          <devices/>
          <address>
            <entry name="local-host1">
              <ip-netmask>4.23.42.5/32</ip-netmask>
              <tag>
                <member>test.tag</member>
                <member>grp.local-group1</member>
              </tag>
            </entry>
          </address>
          <address-group>
            <entry name="local-group1">
              <static>
                <member>local-host1</member>
              </static>
            </entry>
          </address-group>
          <tag>
            <entry name="test.tag"/>
          </tag>
          <pre-rulebase>
            <security>
              <rules>
                <entry name="perimeter - allow users outside">
                  <from>
                    <member>internal</member>
                  </from>
                  <to>
                    <member>external</member>
                  </to>
                  <source>
                    <member>any</member>
                  </source>
                  <destination>
                    <member>any</member>
                  </destination>
                  <source-user>
                    <member>known-user</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <application>
                    <member>any</member>
                  </application>
                  <service>
                    <member>application-default</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>allow</action>
                </entry>
              </rules>
            </security>
            <decryption>
              <rules>
                <entry name="dv1-pre decrypt 1">
                  <category>
                    <member>any</member>
                  </category>
                  <type>
                    <ssl-forward-proxy/>
                  </type>
                  <from>
                    <member>any</member>
                  </from>
                  <to>
                    <member>any</member>
                  </to>
                  <source>
                    <member>any</member>
                  </source>
                  <destination>
                    <member>any</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <action>no-decrypt</action>
                </entry>
              </rules>
            </decryption>
          </pre-rulebase>
          <post-rulebase>
            <security>
              <rules>
                <entry name="no traffic out of dc">
                  <from>
                    <member>internal</member>
                  </from>
                  <to>
                    <member>any</member>
                  </to>
                  <source>
                    <member>Datacenter Subnet</member>
                  </source>
                  <destination>
                    <member>any</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <application>
                    <member>any</member>
                  </application>
                  <service>
                    <member>application-default</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>deny</action>
                </entry>
              </rules>
            </security>
          </post-rulebase>
        </entry>
        <entry name="Datacenter-Firewalls">
          <devices/>
        </entry>
      </device-group>
      <template>
        <entry name="Template-test">
          <settings>
            <multi-vsys>no</multi-vsys>
            <operational-mode>normal</operational-mode>
            <vpn-disable-mode>no</vpn-disable-mode>
          </settings>
          <config>
            <devices>
              <entry name="localhost.localdomain">
                <vsys>
                  <entry name="vsys1"/>
                </vsys>
              </entry>
            </devices>
          </config>
        </entry>
      </template>
    </entry>
  </devices>
</config>
